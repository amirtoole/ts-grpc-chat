{
  "compilerOptions": {
    "lib": [
      "es6"
    ],
    "module": "commonjs",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "preserveConstEnums": true,
    "target": "es2017",
    "noEmit": true,
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules"
    ],
    "types": [
      "node"
    ]
  },
  "include": [
    "src",
    "test",
    "proto-gen"
  ]
}